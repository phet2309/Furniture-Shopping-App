up className=\"mb-3\" controlId=\"email\">\n          <Form.Label>Email Address</Form.Label>\n          <Form.Control type=\"email\" placeholder=\"Enter your email\"\n            value={email}\n            onChange={(e) => setEmail(e.target.value)} />\n        </Form.Group>\n        <Form.Group className=\"mb-3\" controlId=\"password\">\n          <Form.Label>Password</Form.Label>\n          <Form.Control type=\"password\" placeholder=\"Password\"\n            value={password}\n            onChange={(e) => setPassword(e.target.value)} />\n        </Form.Group>\n        <Button variant=\"primary\" type=\"submit\">\n          Submit\n        </Button>\n      </Form>\n    </FormContainer>\n  )\n}\n\nexport default SignupScreen;","/Users/hetpatel/Documents/NJIT/Fall '22/DMSD/Projects/desksrus/src/components/FormContainer.tsx",[],[],"/Users/hetpatel/Documents/NJIT/Fall '22/DMSD/Projects/desksrus/src/screens/StoreScreen.tsx",[],[],"/Users/hetpatel/Documents/NJIT/Fall '22/DMSD/Projects/desksrus/src/components/ControlledCarousel.tsx",["56"],[],{"ruleId":"57","severity":1,"message":"58","line":1,"column":10,"nodeType":"59","messageId":"60","endLine":1,"endColumn":16},{"ruleId":"57","severity":1,"message":"61","line":4,"column":20,"nodeType":"59","messageId":"60","endLine":4,"endColumn":39},{"ruleId":"57","severity":1,"message":"62","line":1,"column":17,"nodeType":"59","messageId":"60","endLine":1,"endColumn":31},"@typescript-eslint/no-unused-vars","'render' is defined but never used.","Identifier","unusedVar","'RouteComponentProps' is defined but never used.","'SyntheticEvent' is defined but never used."]